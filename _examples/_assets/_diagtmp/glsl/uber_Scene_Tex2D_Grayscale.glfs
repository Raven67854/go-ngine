#version 330 core
#line 1
out vec3 out_Color;
in vec2 var_vec2_Tex2D;
uniform sampler2D uni_sampler2D_Tex2D;

vec3 fx_Tex2D (const in vec3 vCol) {
	return texture(uni_sampler2D_Tex2D, var_vec2_Tex2D).rgb;
}

vec3 fx_Grayscale (const in vec3 vCol) {
	return vec3((vCol.r * 0.3) + (vCol.g * 0.59) + (vCol.b * 0.11));
}

void main () {
	out_Color = fx_Tex2D(out_Color);
	out_Color = fx_Grayscale(out_Color);
}
